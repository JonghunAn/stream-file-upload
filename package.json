  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" & eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\" & eslint \"{src,apps,libs,test}/**/*.ts\"",
    "start": "NODE_ENV=production nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --silent=false --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --verbose --runInBand --detectOpenHandles --forceExit --testTimeout=20000 --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
